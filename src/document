重要的sql

//当前时间的前一天  00：00
select    to_char(trunc(sysdate -1), 'yyyy-mm-dd  hh24:mi:ss') from dual;

//当前时间的前一天 23:59:59
select    to_char((trunc(sysdate))-1/(24*60*60), 'yyyy-mm-dd  hh24:mi:ss') from dual;



//创建序列


-- Create sequence 
create sequence SEQ_COOPERATION_SALE_TABLE
minvalue 1
maxvalue 999999999999999
start with 1
increment by 1
cache 20;



