<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cndym.bean.UserGroup" table="MCY_USERGROUP">
        <id name="groupId" column="GROUP_ID">
            <generator class="sequence">
                <param name="sequence">SEQ_USER_GROUP</param>
            </generator>
        </id>
        <property name="groupName" column="GROUP_NAME" type="java.lang.String" />
        <property name="createTime" column="CREATE_TIME" type="java.util.Date" />
        <property name="groupPermissions" column="GROUP_PERMISSIONS" type="java.lang.String" /> 
    </class>
</hibernate-mapping>