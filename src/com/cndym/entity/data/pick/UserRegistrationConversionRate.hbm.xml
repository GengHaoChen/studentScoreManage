<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cndym.entity.data.pick.UserRegistrationConversionRate" table="USER_REGISTRATION_RATE">
        <id name="id" column="ID">
            <generator class="sequence">
                <param name="sequence">SEQ_USER_REGISTRATION_RATE</param>
            </generator>
        </id>
         <property name="loginStatus" column="LOGIN_STATUS" type="java.lang.Integer" />
         <property name="bettingStatus" column="BETTING_STATUS" type="java.lang.Integer" />
         <property name="isLoginFlag" column="IS_LOGIN_FLAG" type="java.lang.Integer" />
         <property name="registrationTime" column="REGISTRATION_TIME" type="java.util.Date" />
         <property name="endTime" column="END_TIME" type="java.util.Date" />
         <property name="createTime" column="CREATE_TIME" type="java.util.Date" />
   </class>
</hibernate-mapping>