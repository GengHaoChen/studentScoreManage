<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cndym.entity.data.put.Member" table="USER_MEMBER">
        <id name="id" column="ID">
            <generator class="sequence">
                <param name="sequence">SEQ_USER_MEMBER</param>
            </generator>
        </id>
        <property name="userName" column="USER_NAME" type="java.lang.String" length="45"/>
        <property name="userCode" column="USER_CODE" type="java.lang.String" length="20"/>
        <property name="mobile" column="MOBILE" type="java.lang.String" length="20"/>
        <property name="email" column="EMAIL" type="java.lang.String" length="100"/>
        <property name="passWord" column="PASS_WORD" type="java.lang.String" length="32"/>
        <property name="passWordStrength" column="PASS_WORD_STRENGTH" type="java.lang.Integer"/>
        <property name="realName" column="REAL_NAME" type="java.lang.String" length="20"/>
        <property name="cardType" column="CARD_TYPE" type="java.lang.Integer" length="1"/>
        <property name="cardCode" column="CARD_CODE" type="java.lang.String" length="45"/>
        <property name="status" column="STATUS" type="java.lang.Integer" length="2"/>
        <property name="registerIp" column="REGISTER_IP" type="java.lang.String" length="20"/>
        <property name="registerApp" column="REGISTER_APP" type="java.lang.String" length="50"/>
        <property name="loginIp" column="LOGIN_IP" type="java.lang.String" length="20"/>
        <property name="loginTime" column="LOGIN_TIME" type="java.util.Date"/>
        <property name="loginCount" column="LOGIN_COUNT" type="java.lang.Integer"/>
        <property name="createTime" column="CREATE_TIME" type="java.util.Date"/>
        <property name="memberId" column="MEMBER_ID" type="java.lang.Long"/>
        <property name="unionId" column="UNION_ID" type="java.lang.String" length="20"/>
        <property name="memberCount" column="MEMBER_COUNT" type="java.lang.Integer"/>
        <property name="bankName" column="BANK_NAME" type="java.lang.String" length="50"/>
        <property name="bankCode" column="BANK_CODE" type="java.lang.String" length="50"/>
        <property name="birthday" column="BIRTHDAY" type="java.lang.String" length="20"/>
        <property name="sex" column="SEX" type="java.lang.Integer" length="2"/>
        <property name="address" column="ADDRESS" type="java.lang.String" length="500"/>
        <property name="memberType" column="MEMBER_TYPE" type="java.lang.Integer"/>
        <property name="trackUnion" column="TRACK_UNION" type="java.lang.Integer"/>
        <property name="qq" column="QQ" type="java.lang.String" length="50"/>
        <property name="msn" column="MSN" type="java.lang.String" length="50"/>
        <property name="loginApp" column="LOGIN_APP" type="java.lang.String" length="50"/>
        <property name="backup1" column="BACK_UP1" type="java.lang.String" length="250"/>
        <property name="province" column="PROVINCE" type="java.lang.String" length="50"/>
        <property name="city" column="CITY" type="java.lang.String" length="50"/>
        <property name="subBank" column="SUB_BANK" type="java.lang.String" length="50"/>
        <property name="machineCode" column="MACHINE_CODE" type="java.lang.String" length="50"/>
        <property name="softVer" column="SOFT_VER" type="java.lang.String" length="10"/>
    </class>
</hibernate-mapping>