<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cndym.entity.email.UserEmail" table="COOPER_USER">
       <id name="id" column="ID">
			<generator class="sequence">
                <param name="sequence">SEQ_USER_EMAIL</param>
            </generator>
		</id>
        <property name="receiveEmail" column="RECEIVE_EMAIL" type="java.lang.String" length="50"/>
        <property name="receiveCode" column="RECEIVE_CODE" type="java.lang.String" length="50"/>
        <property name="title" column="TITLE" type="java.lang.String" length="200"/>
        <property name="body" column="BODY" type="org.hibernate.type.TextType"/>
        <property name="status" column="STATUS" type="java.lang.Integer" length="1"/>
        <property name="createTime" column="CREATE_TIME" type="java.util.Date"/>
        <property name="sendTime" column="SEND_TIME" type="java.util.Date"/>
    </class>
</hibernate-mapping>