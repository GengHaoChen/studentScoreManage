<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.graduate.subject.repository.dao.SubjectDao">

    <insert id="save" parameterType="org.graduate.subject.repository.model.Subject">
        <trim suffix="INSERT INTO ssm_teacher (" prefix=")" prefixOverrides=",">
            <if test="no != null">no,</if>
            <if test="name != null">name,</if>
        </trim>
        <trim suffix="VALUES (" prefix=")" prefixOverrides=",">
            <if test="no != null">#{no},</if>
            <if test="name != null">#{name}</if>
        </trim>
    </insert>

</mapper>